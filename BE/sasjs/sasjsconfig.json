{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "programFolders": [
    "sasjs/BESASGC"
  ],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common",
      "sasjs/services/files"
    ],
    "initProgram": "sasjs/init.sas"
  },
  "jobConfig": {
    "jobFolders": [
      "sasjs/jobs/admin"
    ],
    "initProgram": "sasjs/init.sas"
  },
  "defaultTarget": "sas9",
  "targets": [
    {
      "name": "server",
      "syncFolder": "sasjs/mocks",
      "serverUrl": "http://localhost:5000",
      "serverType": "SASJS",
      "appLoc": "/Boleta/migracion",
      "streamConfig": {
        "streamLogo": "assets/img/logo.jpg",
        "streamServiceName": "Boleta_Electronica",
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "dist/BE"
      }
    },
    {
      "name": "sas9",
      "serverUrl": "https://sasdesa.sii.cl:8343",
      "serverType": "SAS9",
      "appLoc": "/Boleta/migracion/sas9",
      "serverName": "SASAppGuide",
      "repositoryName": "SASAppEGRC",
      "streamConfig": {
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "dist/BE"
      }
    }
  ]
}